{
  "name": "student-dashboard-backend",
  "version": "1.0.0",
  "description": "Backend API for Student Dashboard",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "test": "jest",
    "seed": "ts-node src/database/seeders/seed.ts",
    "db:reset": "rm -f database.sqlite && npm run seed",
    "build:prod": "NODE_ENV=production npm run build",
    "start:prod": "NODE_ENV=production npm start",
    "seed:prod": "NODE_ENV=production npm run seed"
  },
  "keywords": [
    "express",
    "typescript",
    "student",
    "education",
    "api"
  ],
  "author": "Student Dashboard Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "@types/validator": "^13.15.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
